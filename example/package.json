{
  "name": "todoapp",
  "version": "1.0.0",
  "description": "Todoapp sample for Radius",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "tsc": "tsc",
    "parcel": "parcel build src/www/js/main.ts -d dist/www/js --public-url /js/",
    "build": "npm-run-all clean tsc copy-assets parcel",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.9",
    "@types/materialize-css": "^1.0.8",
    "@types/mongodb": "^3.6.11",
    "@types/node": "^14.14.37",
    "@types/shelljs": "^0.8.8",
    "@types/vue": "^2.0.0",
    "fs-extra": "^9.1.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@azure/identity": "^1.3.0",
    "@azure/keyvault-secrets": "^4.1.0",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.2",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "materialize-css": "^1.0.0",
    "mongodb": "^3.6.5",
    "npm": "^7.7.6",
    "package.json": "^2.0.1",
    "uuid": "^3.4.0",
    "vue": "^2.6.12"
  }
}
