{
  "name": "test-env",
  "properties": {
    "compute": {
      "kind": "kubernetes",
      "namespace": "test-namespace"
    },
    "recipeConfig": {
      "terraform": {
        "authentication": {
          "git": {
            "pat": {
              "github.com": {
                "secret": "/planes/radius/local/resourcegroups/test-group/providers/Applications.Core/secretStores/github-pat"
              },
              "gitlab.com": {
                "secret": "/planes/radius/local/resourcegroups/test-group/providers/Applications.Core/secretStores/gitlab-pat"
              }
            }
          }
        },
        "providerMirror": {
          "url": "https://terraform.example.com",
          "providerMappings": {
            "registry.terraform.io/hashicorp/aws": "terraform.example.com/providers/aws"
          },
          "authentication": {
            "token": {
              "secret": "/planes/radius/local/resourcegroups/test-group/providers/Applications.Core/secretStores/registry-token"
            },
            "additionalHosts": ["gitlab.com", "packages.gitlab.com"]
          }
        },
        "version": {
          "version": "1.7.0",
          "releasesApiBaseUrl": "https://releases.example.com",
          "authentication": {
            "token": {
              "secret": "/planes/radius/local/resourcegroups/test-group/providers/Applications.Core/secretStores/releases-token"
            }
          }
        }
      }
    }
  }
}
