{
  "properties": {
    "connections": {
      "gateway": {
        "kind": "UnknownKind",
        "anotherThing": true,
        "source": "id"
      },
      "backend": {
        "kind": "Http",
        "source": "id"
      },
      "frontend": {
        "kind": "Http",
        "source": "something",
        "anotherThing": true
      }
    },
    "container": {
      "image": 45,
      "env": {
        "TEST": "Value",
        "COol?": true
      },
      "ports": {
        "web": {
          "provides": "something",
          "protocol": "TCP"
        },
        "grpc": {
          "containerPort": 80,
          "provides": "something",
          "protocol": "GRPCLOL"
        }
      },
      "readinessProbe": {
        "kind": "tcp",
        "containerPort": 8080,
        "kind": "exec",
        "command": "abc",
        "initialDelaySeconds": 3,
        "failureThreshold": 30,
        "periodSeconds": 3
      },
      "livenessProbe": {
        "kind": "httpGet",
        "containerPort": 8080,
        "initialDelaySeconds": 3,
        "failureThreshold": 30,
        "periodSeconds": 3
      },
      "volumes": {
        "vol1": {
          "kind": "ephemeral"
        },
        "vol2": {
          "kind": "persistent",
          "rbac": "read",
          "mountPath": "/mnt/images"
        },
        "vol3": {
          "kind": "bad",
          "mountPath": "/tmp",
          "managedStore": "memory"
        },
        "vol4": {
          "kind": "persistent",
          "rbac": "rw",
          "source": "a",
          "mountPath": "/tmp"
        },
        "vol5": {
          "kind": "ephemeral",
          "mountPath": "/tmp",
          "managedStore": "bad"
        },
        "vol6": {
          "mountPath": "/tmp",
          "managedStore": "disk"
        }
      }
    },
    "traits": [
      {
        "kind": "UNKNOWN TRAIT",
        "appId": "cool-app"
      },
      {
        "kind": "dapr.io/Sidecar@v1alpha1"
      },
      {
        "appId": "foo"
      }
    ]
  }
}
