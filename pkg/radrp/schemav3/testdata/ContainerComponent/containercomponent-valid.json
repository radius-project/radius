{
  "properties": {
    "connections": {
      "backend": {
        "kind": "Http",
        "source": "something"
      }
    },
    "container": {
      "image": "something",
      "env": {
        "TEST": "Value"
      },
      "ports": {
        "web": {
          "containerPort": 80,
          "provides": "something",
          "protocol": "TCP"
        }
      },
      "readinessProbe": {
        "probeDetails": {
          "kind": "httpGet",
          "path": "/healthz",
          "containerPort": 8080,
          "headers": {
              "Custom-Header": "Hello World"
          }
        },
        "initialDelaySeconds": 3,
        "failureThreshold": 30,
        "periodSeconds": 3
      }
    },
    "liveness": {
      "probeDetails": {
        "kind": "tcp",
        "containerPort": 8080
      }
    }
  },
  "traits": [
    {
      "kind": "dapr.io/App@v1alpha1",
      "appId": "cool-app"
    }
  ]
}