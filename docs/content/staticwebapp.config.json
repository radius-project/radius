{
    "routes": [
        {
            "route": "/.auth/login/aad",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/.auth/logout",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/.auth/invitations",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/.auth/login/twitter",
            "statusCode": 401
        },
        {
            "route": "/.auth/login/github",
            "statusCode": 401
        },
        {
            "route": "/*",
            "allowedRoles": ["authenticated"]
        }
    ],
    "responseOverrides": {
        "401": {
            "redirect": "/.auth/login/aad",
            "statusCode": 302
        }
    }
}