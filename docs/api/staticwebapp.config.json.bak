{
    "routes": [
        {
            "route": "/login",
            "rewrite": "/.auth/login/github"
        },
        {
            "route": "/logout",
            "serve": "/.auth/logout"
        },
        {
            "route": "/.auth/login/github",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/.auth/logout",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/*",
            "allowedRoles": ["reader"]
        }
    ],
    "responseOverrides": {
        "401": {
            "redirect": "/login",
            "statusCode": 302
        }
    }
}
